    <script type="text/javascript">
        $(function () {
            $(".customizer-links").addClass('d-none');

            
            var start = moment().subtract(29, 'days');
            var end = moment();

            function cb(start, end) {
                $('#reportrange span').html(start.format('D MMMM, YYYY') + ' - ' + end.format('D MMMM, YYYY'));
            }

            $('#reportrange').daterangepicker({

                startDate: start,
                endDate: end,
                show: true,
                ranges: {
                    'Today': [moment(), moment()],
                    'Yesterday': [moment().subtract(1, 'days'), moment().subtract(1, 'days')],
                    'Last 7 Days': [moment().subtract(6, 'days'), moment()],
                    'Last 30 Days': [moment().subtract(29, 'days'), moment()],
                    'This Month': [moment().startOf('month'), moment().endOf('month')],
                    'Last Month': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')]
                }
            }, cb);
            cb(start, end);

            AllowMenu();
        });

        function AllowMenu() {

			try {
				$.ajax({
					type: "Post",
					contentType: "application/x-www-form-urlencoded; charset=UTF-8",
					url: "/User/GetAllowMenu",
					dataType: "json",
					async: false,
					success: function (data) {

                        $(".securityLi").find('li').each(function () {
                            var showmenu;
                            var subMenuId = $(this).attr('id');
                            if (subMenuId != "0") {

                                $.each(data, function (i, item) {
                                    if (subMenuId == item.menuId) {
                                        showmenu = true;
                                    }
                                });

                                if (showmenu == true) {
                                    $(this).show();
                                }
                                else {
                                    $(this).remove();
                                }
                            }
                        });

						$(".securityLi").each(function () {
							if($.trim($(this).find('ul').html()) == ""){
								$(this).remove();
							}
						})

					},
					error: function (data) {
						alert(data);
					}
				});
			}
			catch (err) {
				alert(err)
			}
		}

        $("body").on('click', ".dateTimeParam", function () {
            var currentDateTime = moment().format('YYYY-MM-DD HH:mm:ss');
            var originalHref = $(this).attr('href');
            var updatedHref = originalHref + (originalHref.includes('?') ? '&' : '?') + "activityLogDateTime=" + currentDateTime;
            $(this).attr('href', updatedHref);
        });

    </script>
</body>
</html>
