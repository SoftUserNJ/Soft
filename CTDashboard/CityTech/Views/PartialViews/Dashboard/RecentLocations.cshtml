@{
    Layout = null;
}
<div style="float: left; width: 100%;">
<h3 style="float: left;">Recent locations</h3>
</div>
<hr style="clear: both;">

<div style="float: left; width: 100%;">
    <table style="width: 100%;" >
        <tbody id="tblrecentlocation">
        </tbody>
    </table>
</div>




<script>

    

    function RecentLocationsWidgetFunPartial(fromDate, toDate, location, objects) {
 
        try {
            $.ajax({
                type: "Get",
                contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                url: "/Home/RecentLocations/",
                dataType: "json",
                async: false,
                data: { fromDate: fromDate, toDate: toDate, location: location, objects: objects },
                success: function (data) {
                    $("#tblrecentlocation").empty();
                    $.each(data, function (i, item) {

                        $("#tblrecentlocation").append(`<tr style = "border-bottom: 1px solid  DIMGREY;">
                                        <td style="float: left;">${item.LOCATION}</td >
                                        </tr>`)
                    });
                    
                  
                },
                error: function (data) {
                }
            });
        } catch (err) {
            console.error(err);
        }
    }

 

    RecentLocationsWidgetFunPartial('@ViewBag.FromDate', '@ViewBag.ToDate', '@ViewBag.Location', '@ViewBag.Objects');
                   // setInterval(RecentLocationsWidgetFunPartial, 5000);
</script>

