@{
    Layout = null;
}

<div class="modal-dialog" style="max-width:800px;">
    <div class="modal-content">
        @*<div class="modal-header p-2 align-items-center">
        <h5 class="modal-title">User Types</h5>
        <button type="button" class="close fs-30" data-bs-dismiss="modal" aria-label="Close">&times;</button>
        </div>*@
        <div class="modal-body p-0">
            <div class="card mb-0">
                <div class="card-body p-3">
                    <div class="d-flex">
                        <h3>Locations</h3>
                        <button type="button" class="btn-close1 ms-auto" data-bs-dismiss="modal"></button>
                    </div>
                    <hr style="margin-top: 0;margin-bottom: 10px;">
                    <div class="row">

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Location</label>
                            <input type="text" class="form-control rounded-pill" id="txtObjectLocation" skinid="Enter Object Location....!">
                            <input type="hidden" id="txtObjectLocationId">
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Location2</label>
                            <input type="text" class="form-control rounded-pill" id="txtObjectLocation2" skinid="Enter Object Location2....!">
                        </div>

                        <div class="col-lg-4 col-md-4 col-sm-12">
                            <div class="form-group mb-2">
                                <label>Station</label>
                                <select class="js-example-basic-single select2" id="drpLocationStation" skinid="Select Station....!"></select>
                            </div>
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Contact Person</label>
                            <input type="text" class="form-control rounded-pill" id="txtLocationContactPerson" skinid="Enter Contact Person....!">
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Person Phone</label>
                            <input type="text" class="form-control rounded-pill" id="txtLocationPersonPhone" skinid="Enter Person Phone....!">
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Residence</label>
                            <input type="text" class="form-control rounded-pill" id="txtLocationResidence" skinid="Enter Residence....!">
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Region</label>
                            <input type="text" class="form-control rounded-pill" id="txtLocationRegion" skinid="Enter Region....!">
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Postal Code</label>
                            <input type="text" class="form-control rounded-pill" id="txtLocationPostalCode" skinid="Enter Postal Code....!">
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Latitude</label>
                            <input type="text" class="form-control rounded-pill" id="txtLocationLatitude" skinid="Enter Latitude....!">
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Longitude</label>
                            <input type="text" class="form-control rounded-pill" id="txtLocationLongitude" skinid="Enter Longitude....!">
                        </div>

                        <div class="form-group col-md-4 col-lg-4 col-sm-12 mb-2">
                            <label class="form-label">Search Location</label>
                            <input type="text" class="form-control rounded-pill" id="txtSearchObjectLocation" skinid="">
                        </div>

                    </div>
                    <div class="mb-2">
                        <button class="ct-btns btn btn-primary btn-sm" id="btnNewObjectLocation">New</button>
                        <button class="ct-btns btn btn-success btn-sm" id="btnSaveObjectLocation">Save</button>
                        <button class="ct-btns btn btn-secondary btn-sm" id="btnRefreshObjectLocation">Refresh</button>
                    </div>
                    <div class="card mb-0 pt-2 pb-1 scroll">
                        <div class="table-responsive p-2 pt-0 pb-0">
                            <table class="table text-white table-hover">
                                <thead>
                                    <tr>
                                        <th class="ps-1 partial-head-leftr">Location</th>
                                        <th class="">Loation2</th>
                                        <th class="">Station</th>
                                        <th class="">Customer</th>
                                        <th class="">ContactPerson</th>
                                        <th class="">PersonPhone</th>
                                        <th class="">Residence</th>
                                        <th class="">Region</th>
                                        <th class="">PostalCode</th>
                                        <th class="">Lati</th>
                                        <th class="">Longi</th>
                                        <th class="text-center partial-head-rightr last-th-sticky">Actions</th>
                                    </tr>
                                </thead>
                                <tbody id="objectLocationList">
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

